<System xmlns="http://systemjtechnology.com">
    <SubSystem Name="SS5" Local="true">
        <ClockDomain Name="ConveyorCD" Class="ConveyorController">

            <!-- Input Signals -->
            <iSignal Name="bottleAtPos1"      Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10004" />
            <iSignal Name="bottleLeftPos5"    Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10004" />
            <iSignal Name="bottleDeployed"    Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10004" />

            <!-- Output Signals -->
            <oSignal Name="motConveyorOnOff"  To="ConveyorPlant.motConveyorOnOff" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10005" />
		
			 <!-- Visualization Signals -->
            <oSignal Name="bottleDeployedE" To="UI.bottleDeployedE" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
            
            
			<iChannel Name="rotated"     From="RotaryCD.rotated" />	
			
		
            <oChannel Name="convReady"     To="RotaryCD.convReady" />
            <oChannel Name="simFiller"     To="FillerCD.simFiller" />
        </ClockDomain>
        
        
        
        
        
        <ClockDomain Name="RotaryCD" Class="RotaryController">

            <!-- Input Signals -->
            <iSignal Name="tableAlignedWithSensor" 	Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10002" />
            <iSignal Name="bottleAtPos5" 			Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10002" />
            <iSignal Name="capOnBottleAtPos1"  		Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10002" />
            
            <!-- Output Signals -->
            <oSignal Name="rotaryTableTrigger" 		To="RotaryPlant.rotaryTableTrigger"	Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10003" />
            
            <!-- Input Channels -->
            <iChannel Name="convReady"     			From="ConveyorCD.convReady" />
			<iChannel Name="fillerReady"     From="FillerCD.fillerReady" />
			
			<oChannel Name="rotated"     To="ConveyorCD.rotated" />		

        </ClockDomain>
        
        
        <ClockDomain Name="FillerCD" Class="FillerController">

          	<!-- Input Signals -->

			<iSignal Name="dosUnitEvac"  Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10006" />
			<iSignal Name="dosUnitFilled"  	Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10006" />

            <!-- Output Signals -->
     		<oSignal Name="valveInjectorOnOff"  To="FillerPlant.valveInjectorOnOff" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10007" />
     		<oSignal Name="valveInletOnOff"		To="FillerPlant.valveInletOnOff" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10007" />
   			<oSignal Name="dosUnitValveRetract" To="FillerPlant.dosUnitValveRetract" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10007" />
      		<oSignal Name="dosUnitValveExtend" 	To="FillerPlant.dosUnitValveExtend" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10007" />
      		
      		<oSignal Name="bottleAtPos2E" 		To="UI.bottleAtPos2E" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
      		
      		<oChannel Name="fillerReady"     To="RotaryCD.fillerReady" />
      		<iChannel Name="simFiller"     From="ConveyorCD.simFiller" />
        </ClockDomain>
        
        
    </SubSystem>
</System>

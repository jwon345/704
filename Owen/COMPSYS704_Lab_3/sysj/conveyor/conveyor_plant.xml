		<iSignal Name="removeBottle" Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10001" />

		<iSignal Name="motorOn" Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10001" />


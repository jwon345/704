<System xmlns="http://systemjtechnology.com">
    <SubSystem Name="SS6" Local="true">
        <ClockDomain Name="ConveyorPlant" Class="ConveyorPlant">
			
            <!-- Input Signals -->
 			<iSignal Name="motConveyorOnOff" Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10005" />

            <!-- Output Signals -->
            <oSignal Name="bottleAtPos1" 		To="ConveyorCD.bottleAtPos1" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10004" />
            <oSignal Name="bottleLeftPos5" 		To="ConveyorCD.bottleLeftPos5" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10004" />
           
          	<oSignal Name="motConveyorOnOffE" 	To="UI.motConveyorOnOffE" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="bottleLeftPos5E"  	To="UI.bottleLeftPos5E"  Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			
        </ClockDomain>
        
        
        
        
       <ClockDomain Name="RotaryPlant" Class="RotaryPlant">
			
            <!-- Input Signals -->
 			<iSignal Name="rotaryTableTrigger" 		Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10003" />

            <!-- Output Signals -->
            <oSignal Name="tableAlignedWithSensor" 	To="RotaryCD.tableAlignedWithSensor" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10002" />
            <oSignal Name="bottleAtPos5" 			To="RotaryCD.bottleAtPos5" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10002" />
            <oSignal Name="capOnBottleAtPos1" 		To="RotaryCD.capOnBottleAtPos1" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10002" />
            
            <!-- Visualization Signals -->
            <oSignal Name="tableAlignedWithSensorE" To="UI.tableAlignedWithSensorE" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />            
            <oSignal Name="capOnBottleAtPos1E" 		To="UI.capOnBottleAtPos1E" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
            

        </ClockDomain>
        
        
        
        
        
        <ClockDomain Name="FillerPlant" Class="FillerPlant">

           	<!-- Input Signals -->
           	<iSignal Name="valveInjectorOnOff"   	Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10007" />
     		<iSignal Name="valveInletOnOff"			Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10007" />
   			<iSignal Name="dosUnitValveRetract" 	Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10007" />
      		<iSignal Name="dosUnitValveExtend" 		Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10007" />


            <!-- Output Signals -->
           
			<oSignal Name="dosUnitEvac"  			To="FillerCD.dosUnitEvac" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10006" />
			<oSignal Name="dosUnitFilled"  			To="FillerCD.dosUnitFilled" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10006" />
			
			  <!-- Visualization Signals -->
            <oSignal Name="valveInjectorOnOffE" 	To="UI.valveInjectorOnOffE" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
            <oSignal Name="valveInletOnOffE" 		To="UI.valveInletOnOffE" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
            <oSignal Name="dosUnitValveRetractE" 	To="UI.dosUnitValveRetractE" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
            <oSignal Name="dosUnitValveExtendE" 	To="UI.dosUnitValveExtendE" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
          
            <oSignal Name="dosUnitEvacE" 			To="UI.dosUnitEvacE" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
            <oSignal Name="dosUnitFilledE" 			To="UI.dosUnitFilledE" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
           	
        </ClockDomain>
        
        
        <ClockDomain Name="CapperPlant" Class="CapperPlant">

			<!-- Input Signals -->
			<iSignal Name="cylPos5ZaxisExtend" 		Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10009" />
			<iSignal Name="gripperTurnRetract"  	Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10009" />
			<iSignal Name="gripperTurnExtend"      	Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10009" />
			<iSignal Name="capGripperPos5Extend"    Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10009" />
			<iSignal Name="cylClampBottleExtend"    Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10009" />

			<!-- Output Signals -->
			
			<oSignal Name="gripperZAxisLowered"     To="CapperCD.gripperZAxisLowered"    Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10008" />
			<oSignal Name="gripperZAxisLifted"    	To="CapperCD.gripperZAxisLifted"    	Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10008" />
			<oSignal Name="gripperTurnHomePos"    	To="CapperCD.gripperTurnHomePos"   	Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10008" />
			<oSignal Name="gripperTurnFinalPos"    	To="CapperCD.gripperTurnFinalPos"   	Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10008" />

			<!-- Visualization Signals -->
			<oSignal Name="cylPos5ZaxisExtendE" 		To="UI.cylPos5ZaxisExtendE" 		Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="gripperTurnRetractE"  	To="UI.gripperTurnRetractE"  		Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="gripperTurnExtendE"      To="UI.gripperTurnExtendE"      		Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="capGripperPos5ExtendE"   To="UI.capGripperPos5ExtendE"   		Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="cylClampBottleExtendE"   To="UI.cylClampBottleExtendE"   		Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="gripperZAxisLoweredE"    To="UI.gripperZAxisLoweredE"    	Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="gripperZAxisLiftedE"    	To="UI.gripperZAxisLiftedE"    		Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="gripperTurnHomePosE"    	To="UI.gripperTurnHomePosE"    		Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="gripperTurnFinalPosE"    To="UI.gripperTurnFinalPosE"    		Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />


			
		</ClockDomain>
		
		
		<ClockDomain Name="PlantCD" Class="Plant">
			<iSignal Name="pusherExtend" Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10001" />
			<iSignal Name="vacOn"        Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10001" />
			<iSignal Name="armSource"    Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10001" />
			<iSignal Name="armDest"      Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10001" />
			<iSignal Name="enable"       Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10001" />
			<iSignal Name="refill"       Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10001" />

			<oSignal Name="pusherRetracted" To="ControllerCD.pusherRetracted" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10000" />
			<oSignal Name="pusherExtended"  To="ControllerCD.pusherExtended"  Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10000" />
			<oSignal Name="WPgripped"       To="ControllerCD.WPgripped"       Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10000" />
			<oSignal Name="armAtSource"     To="ControllerCD.armAtSource"     Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10000" />
			<oSignal Name="armAtDest"       To="ControllerCD.armAtDest"       Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10000" />
			<oSignal Name="empty"           To="ControllerCD.empty"           Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10000" />

			<oSignal Name="pusherRetractedE" To="UI.pusherRetractedE" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="pusherExtendedE"  To="UI.pusherExtendedE"  Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="WPgrippedE"       To="UI.WPgrippedE"       Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="armAtSourceE"     To="UI.armAtSourceE"     Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="armAtDestE"       To="UI.armAtDestE"       Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="emptyE"           To="UI.emptyE"           Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
		</ClockDomain>
		
		
		
    </SubSystem>
</System>
